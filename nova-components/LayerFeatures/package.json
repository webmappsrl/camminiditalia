{
  "name": "wm/layer-features",
  "description": "Layer Features Nova Component",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0"
  },
  "autoload": {
    "psr-4": {
      "Wm\\LayerFeatures\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wm\\LayerFeatures\\LayerFeaturesServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "dev": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "mix --production"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vue/compiler-sfc": "^3.2.31",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^8.0.0",
    "hash-sum": "^2.0.0",
    "laravel-mix": "^6.0.49",
    "laravel-nova-devtool": "file:vendor/laravel/nova-devtool",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.32.8",
    "sass-loader": "^12.1.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.9.5",
    "vue": "^3.2.31",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.6.14"
  },
  "dependencies": {
    "ag-grid-community": "^30.2.1",
    "ag-grid-vue3": "^30.2.1",
    "laravel-nova": "^1.12.3"
  }
}
